        <name>Generate PDF</name>
        <onclick/>
        <order>100</order>
        <script><![CDATA[(function execute() {
    try {
        // 创建PDF文档
        var pdf = new PDFDocument();
        pdf.setTitle(current.getDisplayValue('name'));
        pdf.addParagraph('Catalog Item: ' + current.getDisplayValue('name'));
        pdf.addParagraph('Description: ' + current.getDisplayValue('description'));
        pdf.addParagraph('Created: ' + current.getDisplayValue('sys_created_on'));

        // 添加更多内容...

        // 生成PDF附件
        var fileName = current.getDisplayValue('name') + '.pdf';
        var attachment = pdf.generate(fileName);

        // 提供下载链接
        action.setRedirectURL(attachment.getURL());
    } catch (e) {
        gs.addErrorMessage('Failed to generate PDF: ' + e.getMessage());
    }
})();]]></script>
