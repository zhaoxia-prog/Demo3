        <script><![CDATA[var GetPreData = Class.create();
GetPreData.prototype = Object.extendsObject(global.AbstractAjaxProcessor, {
    getPreviousRequestData: function() {
        var reqId = this.getParameter('sysparm_reqId');
        var reqItem = new GlideRecord('sc_req_item');
        reqItem.get(reqId);
		var result = {};
		for(var key in reqItem.variables){
			result[key] = reqItem.variables[key] + '';
		}
        return JSON.stringify(result);
    },
    type: 'GetPreData'
});]]></script>
